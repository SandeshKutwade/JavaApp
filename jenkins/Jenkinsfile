pipeline{
    agent{label "javanode"}
    tools {maven 'maventool'}

    stages{
        stage('Build'){
            steps{
                sh 'mvn clean install'
            }
        }

        stage('Build docker image'){
                agent{label "javanode"}
                    steps{
                        script{
                            sh 'docker build -t JavaApp .'
                        }
                    }
        }
    }
}