pipeline{
    agent(label "javanode")
    tools {maven 'maventool'}

    stages{
        stage('compile'){
            steps{
                sh 'mvn compile'
            }
        }
        stage("test") {
            steps{
                sh "mvn test"

            }
        }
        stage{"Package"} {
            steps{
                sh "mvn package"
            }
        }
    }
}